{
    "manifest_version": 2,
    "name": "Pomie Open Membership Undepresso",
    "version": "0.9",
    "description": "An attempt to fill the empty boxes of depression in vods of pomu's channel after the membership is closed",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  